---
import BagiKelompok from '../../components/simplePrograms/BagiKelompok';
import Breadcrumbs from '../../components/Breadcrumbs.astro';
import Layout from '../../layouts/Layout.astro';
import Container from '../../components/Container.astro';

const maintenance = import.meta.env.MAINTENANCE;

if (maintenance === '1') {
	return Astro.redirect('/maintenance');
}
---

<Layout
	title="Bagi kelompok acak - Andndre"
	desc="Program sederhana untuk membagi anggota menjadi beberapa kelompok secara acak"
>
	<Breadcrumbs />
	<h1>Bagi Kelompok Acak</h1>
	<br />
	<br />
	<Container>
		<BagiKelompok client:load />
	</Container>
</Layout>
